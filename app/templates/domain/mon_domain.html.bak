    <style type="text/css">
            #title{
                margin:4px;
            }
    </style>
    <style type="text/css">
    table.gridtable {
        width:95%;
        font-family: verdana,arial,sans-serif;
        color:#333333;
        border-width: 1px;
        border-color: #666666;
        border-collapse: collapse;
    }
    table.gridtable th {
        border-width: 1px;
        padding: 8px;
        border-style: solid;
        border-color: #666666;
        /*background-color: #99BBFF;*/
        font-size:12px;
        text-align:center;
    }
    table.gridtable td {
        border-width: 1px;
        padding: 8px;
        border-style: solid;
        border-color: #666666;
        /*background-color: #ffffff;*/
        font-size:11px;
        text-align:center;
    }
    </style>
</head>
    <body>  
       <!--选项卡-->
        <form  action=/domain_mon method="POST" name="choose" >
	&nbsp;&nbsp;&nbsp;&nbsp;<input value="全部" name="submit" type="submit">>>><input value="一号站" name="submit" type="submit">---<input value="二号站" name="submit" type="submit">---<input value="市场" name="submit" type="submit">---<input value="穿云梯" name="submit" type="submit"> 
        </form>
	<div id="tab">  
            <!--选项的头部-->  
            <div id="tab-header">  
                <ul>  
                    {% for loc in l_loc %}
                        {% if loc == '深圳' %}
                            <li class="selected">{{loc}}</li>  
                        {% else %}
                            <li>{{loc}}</li>
                        {% endif %}
                    {% endfor %}
                </ul>  
            </div>  
            <!--主要内容-->  
            <div id="tab-content">  
                {% for x in l_all %}
                    {% if x[0] == '深圳' %}
                        <div class="dom" style="display: block;"  ><br>
                            <table class="gridtable">
                                <tr>
                                    <th align="center">地区</th><th>域名</th><th>打开率</th><th>DNS解析时间</th><th>打开时间</th><th>返回码</th>
                                </tr>  
                                {% if x[1][0][1]>60 and (x[1][0][4] == '200' or x[1][0][4] == '302') %}                         
                                <tr style="color:blue">
                                {% else %}
                                <tr style="color:red">   
                                {% endif %}  
                                    <td rowspan={{x[1]|count}}>{{x[0]}}</td><td>{{x[1][0][0]}}</td><td>{{x[1][0][1]}}%</td><td>{{x[1][0][2]}}</td><td>{{x[1][0][3]}}秒</td><td>{{x[1][0][4]}}</td>
                                </tr>
                            {% for y in x[1] %}
                                {% if loop.index0>0 %}
                                    {% if y[1]>60 and (y[4] == '200' or y[4] == '302') %}
                                    <tr style="color:blue"> 
                                     {% else %}
                                    <tr style="color:red">   
                                    {% endif %}                       
                                        <td>{{y[0]}}</td><td>{{y[1]}}%</td><td>{{y[2]}}</td><td>{{y[3]}}秒</td><td>{{y[4]}}</td>  
                                    </tr>
                                {% endif %}
                            {% endfor %}
                            </table>
                        </div>
                    {% else %}
                        <div class="dom">  
                        <br>
                        <table class="gridtable">
                                <tr>
                                    <th align="center">地区</th><th>域名</th><th>打开率</th><th>DNS解析时间</th><th>打开时间</th><th>返回码</th>
                                </tr>
                                {% if x[1][0][1]>60 and (x[1][0][4] == '200' or x[1][0][4] == '302') %}                         
                                <tr style="color:blue">
                                {% else %}
                                <tr style="color:red">   
                                {% endif %}  
                                    <td rowspan={{x[1]|count}}>{{x[0]}}</td><td>{{x[1][0][0]}}</td><td>{{x[1][0][1]}}%</td><td>{{x[1][0][2]}}</td><td>{{x[1][0][3]}}秒</td><td>{{x[1][0][4]}}</td>
                            </tr>
                            {% for y in x[1] %}
                                {% if loop.index0>0 %}
                                 {% if y[1]>60 and (y[4] == '200' or y[4] == '302') %}
                                <tr style="color:blue"> 
                                {% else %}
                                <tr style="color:red">   
                                {% endif %}           
                                    <td>{{y[0]}}</td><td>{{y[1]}}%</td><td>{{y[2]}}</td><td>{{y[3]}}秒</td><td>{{y[4]}}</td>  
                                </tr>
                                {% endif %}
                            {% endfor %}
                        </table>
                        </div>  
                    {% endif %}
                {% endfor %}
            </div>  
        </div>  
