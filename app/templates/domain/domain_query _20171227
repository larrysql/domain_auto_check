      // cbs[i].checked=true;
      // }
      // }
      signal = "{{signal}}";
      if(signal=='update_domain')
      {
        alert("修改成功!");
      };
      if(signal=='delete_domain')
      {
        alert("删除成功!");
      };
      if(signal=='add_domain')
      {
        alert("增加成功!");
      };
      if(signal=='disable_domain')
      {
        alert("禁用成功!");
      };
      if(signal=='enable_domain')
      {
        alert("启用成功!");
      };
    }
    </script>
